{
  "name": "ado-terraform-plan-viewer",
  "version": "1.0.0",
  "description": "Terraform Plan Viewer",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kensykora/ado-terraform-plan-viewer"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifests azure-devops-extension.json",
    "publish-extension": "tfx extension publish --manifests azure-devops-extension.json"
  },
  "dependencies": {
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "~2.164.0",
    "react": "~16.8.1",
    "react-dom": "~16.8.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "file-loader": "~6.2.0",
    "rimraf": "~2.6.2",
    "tfx-cli": "^0.6.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
